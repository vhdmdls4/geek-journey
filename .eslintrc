{
  "env": {
    "node": true,
    "es6": true
  },
    "plugins": [
      "prettier"
    ],
  "rules": {
    "no-undef": "off",
    "no-console": "error",
    "react/display-name": "off",
    "indent": [
      "error",
      2
    ],
    "quotes": ["error", "single"],
    "semi": "off",
    "react/prop-types": "off"
  },
  "extends": [
    "plugin:react/recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "modules": true,
      "experimentalObjectRestSpread": true
    }
  }
}
